{
    "world": {
        "category": "世界",
        "protocol": [
            {
                "description": "既定値の世界IDの取得",
                "method": "GET",
                "action": "getWorldID",
                "names": [
                    {
                        "opt": "me",
                        "dev": "Done"
                    }
                ],
                "result": "text/plain"
            },
            {
                "description": "世界の生成",
                "method": "GET",
                "action": "newWorld",
                "result": "text/plain",
                "dev": "Done"
            },
            {
                "description": "世界の消去",
                "method": "GET",
                "action": "closeWorld",
                "names": [
                    {
                        "opt": "world",
                        "type": "string",
                        "required": "required",
                        "hint": "消去する世界のID",
                        "dev": "Done"
                    }
                ]
            }
        ]
    },
    "param": {
        "category": "パラメータ",
        "protocol": [
            {
                "description": "パラメータ値の取得",
                "method": "GET",
                "action": "getParams",
