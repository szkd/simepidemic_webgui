                    },
                    {
                        "opt": "format",
                        "dev": "対応予定未定",
                        "type": "number",
                        "hint": "整数，JSONフォーマットオプション"
                    },
                    {
                        "opt": "world",
                        "dev": "対応予定未定",
                        "type": "text"
                    },
                    {
                        "opt": "me",
                        "dev": "対応予定未定",
                        "type": "text"
                    }
                ]
            },
            {
                "description": "パラメータ値の設定",
                "method": "POST",
                "action": "setParams",
                "type": "form-data",
                "target": "loadParamResult",
                "result": "iframe",
                "names": [
                    {
                        "opt": "name",
                        "type": "text",
                        "dev": "Done",
                        "required": "required",
                        "hint": "対象の世界ID, 'default'であれば既定値世界を表す"
                    },
                    {
                        "opt": "file",
                        "dev": "Done"
                    }
                ]
            },
            {
                "description": "パラメータ値の設定",
                "method": "POST",
                "action": "setParams",
                "type": "form-data",
                "target": "loadParamResult",
                "result": "iframe",
