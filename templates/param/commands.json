{
    "info": {
        "formname": "param-form",
        "cmd_cls": "command-button",
        "cmd_list_cls": "cmd-btn-list"
    },
    "listener": {
        "save": "saveParams('param-form');",
        "reset": "resetParams();"
    },
    "buttons": {
        "save": "現在の設定を保存",
        "reset": "既定値に戻す"
    },
    "other": {
        "file-label": {
            "tag": "label",
            "content":"ファイルから読み込む",
            "end": "True",
            "attr": {
                "for": "param-title-button",
                "class": "command-button"
            }
        },
        "file-input": {
            "tag": "input",
            "end": "False",
            "attr": {
                "type": "file",
                "id": "param-title-button",
                "class": "command-button",
                "style": "display:none;",
                "accept": ".json, application/json",
                "onchange": "loadFile(this, target='parampanel');"
            }
        }
    }
}
