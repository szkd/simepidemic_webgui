{
    "info": {
        "cmd_cls": "panel-cmd",
        "cmd_list_cls": "panel-cmd-list"
    },
    "listener": {
        "save": "saveForm('ID-form');",
        "reset": "resetForm('ID-form');"
    },
    "buttons": {
        "save": {
            "JA": "現在の設定を保存",
            "EN": "Save current settings"
        },
        "reset": {
            "JA": "既定値に戻す",
            "EN": "Reset"
        }
    },
    "other": {
        "file-label": {
            "tag": "label",
            "content": {
                "JA": "ファイルから読み込む",
                "EN": "Load file"
            },
            "end": "True",
            "attr": {
                "for": "ID-file",
                "class": "panel-cmd"
            }
        },
        "file-input": {
            "tag": "input",
            "end": "False",
            "attr": {
                "type": "file",
                "id": "ID-file",
                "style": "display:none;",
                "accept": ".json, application/json",
                "onchange": "loadForm(this, 'ID-form');"
            }
        }
    }
}
